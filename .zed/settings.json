{
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  // vim_mode is optional: contributor
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "buffer_font_size": 15,
  // theme is optional: contributor
  "project_panel": {
    // dock is optional: contributor
    "auto_fold_dirs": false,
    "entry_spacing": "comfortable",
  },
  "preview_tabs": {
    "enabled": false,
  },
  "tabs": {
    "file_icons": true,
  },
  "git": {
    "inline_blame": {
      "enabled": false,
    },
  },
  "current_line_highlight": "none",
  "tab_size": 4,
  "hard_tabs": true,
  // cursor shape is optional: contributor
  // cursor blink is optional: contributor
  "prettier": {
    "allowd": true,
  },
  "autosave": "off",
  "show_edit_predictions": false,
  "disable_ai": false,
}
