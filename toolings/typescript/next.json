{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "extends": "./base.json",
    "compilerOptions": {
        "plugins": [
            {
                "name": "next"
            }
        ],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowJs": true,
        "jsx": "preserve",
        "noEmit": true
    },
    "include": [],
    "exclude": ["node_modules"]
}
